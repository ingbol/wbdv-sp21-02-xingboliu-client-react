ssages":"86","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"87","messages":"88","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"89","messages":"90","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"91","messages":"92","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"93"},{"filePath":"94","messages":"95","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/index.js",[],["96","97"],"/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/App.js",[],"/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/reportWebVitals.js",[],"/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/components/home.js",["98"],"import React from 'react'\nimport {Link} from 'react-router-dom'\n\nexport default () =>\n    <>\n        <h1>Home</h1>\n        <div className=\"list-group\">\n            <Link to=\"/courses/table\" className=\"list-group-item\">\n                Courses Table\n            </Link>\n            <Link to=\"/courses/grid\" className=\"list-group-item\">\n                Courses Grid\n            </Link>\n            <Link to=\"/editor\" className=\"list-group-item\">\n                Course Editor\n            </Link>\n        </div>\n    </>\n\n\n\n","/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/components/course-manager.js",["99","100"],"import React,{useState} from \"react\";\nimport {Link, Route} from \"react-router-dom\";\nimport courseService from \"../services/course-sevice\"\nimport CourseTable from \"./course-table\";\nimport CourseGrid from \"./course-grid\";\nimport CourseEditor from \"./course-editor/course-editor\";\n\n\nexport default class CourseManager\n    extends React.Component {\n    state = {\n        courses: [],\n    }\n\n    title = ''\n\n    componentDidMount() {\n        courseService.findAllCourses()\n            .then(courses => this.setState({courses}))\n    }\n\n    updateCourse = (course) => {\n        courseService.updateCourse(course._id, course)\n            .then(status => {\n                this.setState((prevState) => {\n                    let nextState = {...prevState}\n                    nextState.courses = prevState.courses.map(c => {\n                        if(c._id === course._id) {\n                            return course\n                        } else {\n                            return c\n                        }\n                    })\n                    return nextState\n                })\n            })\n    }\n\n    deleteCourse = (course) => {\n        courseService.deleteCourse(course._id)\n            .then(status => {\n                this.setState(prevState => ({\n                    courses: prevState.courses.filter(c => c._id !== course._id)\n                }))\n            })\n    }\n\n    addCourse = () => {\n        if(this.title === ''){\n            this.title = 'New Course'\n        }\n        const newCourse = {\n            title: this.title,\n            owner: \"me\",\n            lastModified: new Date().toISOString().split('T')[0]\n        }\n        courseService.createCourse(newCourse)\n            .then(actualCourse => {\n                this.state.courses.push(actualCourse)\n                this.setState(this.state)\n            })\n    }\n\n\n    render() {\n\n        return(\n            <div className=\"wbdv-container\">\n\n                <Route path=\"/courses/table\" exact={true} >\n                    <div className=\"row wbdv-sticky-top wbdv-padding-5px\">\n                        <div className=\"col-1\">\n                            <i className=\"fa fa-bars fa-2x\"></i>\n                        </div>\n                        <div className=\"col-3 d-none d-lg-block\">\n                            <h4>Course Manager</h4>\n                        </div>\n                        <div className=\"col-7\">\n                            <input className=\"form-control\"\n                                   placeholder=\"New Course Title\"\n                                   onChange={(e) => this.title = e.target.value}\n                            />\n                        </div>\n                        <div className=\"col-1\">\n                            <i className=\"fa fa-plus-circle fa-2x\" onClick={this.addCourse}></i>\n                        </div>\n                    </div>\n                    <i className=\"fa fa-plus-circle fa-4x\" onClick={this.addCourse}></i>\n                    <br/>\n                    <br/>\n\n                    <CourseTable\n                        updateCourse={this.updateCourse}\n                        deleteCourse={this.deleteCourse}\n                        courses={this.state.courses}/>\n\n                </Route>\n\n                <Route path=\"/courses/grid\" exact={true} >\n                    <div className=\"row wbdv-sticky-top wbdv-padding-5px\">\n                        <div className=\"col-1\">\n                            <i className=\"fa fa-bars fa-2x\"></i>\n                        </div>\n                        <div className=\"col-3 d-none d-lg-block\">\n                            <h4>Course Manager</h4>\n                        </div>\n                        <div className=\"col-7\">\n                            <input className=\"form-control\"\n                                   placeholder=\"New Course Title\"\n                                   onChange={(e) => this.title = e.target.value}\n                            />\n                        </div>\n                        <div className=\"col-1\">\n                            <i className=\"fa fa-plus-circle fa-2x\" onClick={this.addCourse}></i>\n                        </div>\n                    </div>\n                    <i className=\"fa fa-plus-circle fa-4x\" onClick={this.addCourse}></i>\n                    <br/>\n                    <br/>\n\n                    <CourseGrid courses={this.state.courses}\n                                updateCourse={this.updateCourse}\n                                deleteCourse={this.deleteCourse}/>\n                </Route>\n\n                <Route path={[\n                    \"/courses/:layout/edit/:courseId\",\n                    \"/courses/:layout/edit/:courseId/modules/:moduleId\",\n                    \"/courses/:layout/edit/:courseId/modules/:moduleId/lessons/:lessonId\",\n                    \"/courses/:layout/edit/:courseId/modules/:moduleId/lessons/:lessonId/topics/:topicId\"]}\n                       exact={true}\n                       render={(props) => <CourseEditor {...props}/>}>\n                </Route>\n            </div>\n        )\n    }\n}\n\n","/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/services/course-sevice.js",[],"/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/components/course-table.js",["101"],"import React from \"react\";\nimport CourseRow from \"./course-row\";\nimport {Link} from \"react-router-dom\";\n\nexport default class CourseTable extends\n    React.Component{\n\n        constructor(props) {\n            super(props);\n        }\n\n        render() {\n            return(\n                <div className=\"wbdv-container\">\n                    <div className=\"row border-bottom\">\n                        <div className=\"col-5\">\n                            Title\n                        </div>\n                        <div className=\"col-3 d-none d-sm-block\">\n                            Owned by\n                            <i className=\"fa fa-sort-up\"></i>\n                        </div>\n                        <div className=\"col-3 d-none d-sm-block\">\n                            Last modified\n                        </div>\n                        <div className=\"col-1 text-nowrap\">\n                            <i className=\"fa fa-folder\"></i>\n                            <i className=\"fa fa-sort-alpha-up-alt\"></i>\n                            <Link to=\"/courses/grid\">\n                                <i className=\"fa fa-th\"></i>\n                            </Link>\n\n                        </div>\n                    </div>\n                    {\n                        this.props.courses.map(course =>\n                        <CourseRow\n                            key={course._id}\n                            deleteCourse={this.props.deleteCourse}\n                            updateCourse={this.props.updateCourse}\n                            course={course}\n                            title={course.title}\n                            lastModified={course.lastModified}\n                            owner={course.owner}/>\n                        )\n                    }\n                </div>\n            )\n        }\n}","/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/components/course-row.js",[],"/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/components/course-grid.js",["102"],"import React from 'react'\nimport {Link} from \"react-router-dom\";\nimport CourseCard from \"./course-card\";\n\n\nexport default class CourseGrid extends\n    React.Component {\n\n    constructor(props) {\n        super(props);\n    }\n\n    render() {\n        return (\n            <div className=\"wbdv-container\">\n                <div className=\"row border-bottom wbdv-name-row\">\n                    <div className=\"col-5 d-none d-sm-block\">\n                        Recent Documents\n                    </div>\n                    <div className=\"col-6 d-none d-sm-block\">\n                        Owned by Me\n                    </div>\n                    <div className=\"col-1 text-nowrap\">\n                        <i className=\"fa fa-folder \"></i>\n                        <i className=\"fa fa-sort-alpha-up-alt \"></i>\n                        <Link to=\"/courses/table\">\n                            <i className=\"fa fa-bars\"></i>\n                        </Link>\n                    </div>\n                </div>\n\n                <div className=\"row\">\n                    {\n                        this.props.courses.map(course =>\n                            <CourseCard\n                                key={course._id}\n                                deleteCourse={this.props.deleteCourse}\n                                updateCourse={this.props.updateCourse}\n                                course={course}\n                                title={course.title}\n                                lastModified={course.lastModified}\n                                owner={course.owner}/>)\n                    }\n                </div>\n            </div>)\n    }\n}\n\n\n","/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/components/course-card.js",["103"],"import React, {useState} from 'react'\nimport {Link} from \"react-router-dom\";\n\nconst CourseCard = (\n    {\n        course,\n        deleteCourse,\n        updateCourse\n    }) => {\n    const [editing, setEditing] = useState(false)\n    const [title, setTitle] = useState(course.title)\n\n    const saveCourse = () => {\n        setEditing(false)\n        const newCourse = {\n            ...course,\n            title: title\n        }\n        updateCourse(newCourse)\n    }\n\n    return(\n        <div className=\"card\" style={{width: \"18rem\", margin: \"15px\"}}>\n            <div className=\"wbdv-top-right\">\n                {\n                    editing &&\n                    <i onClick={() => saveCourse()} className=\"fas fa-check\"></i>\n                }\n                <i onClick={() => deleteCourse(course)} className=\"fas fa-times\"></i>\n            </div>\n\n            <div className=\"wbdv-bottom-right\">\n                {\n                    !editing &&\n                    <i onClick={() => setEditing(true)} className=\"fas fa-edit\"></i>\n                }\n            </div>\n\n            <img src=\"https://www.valuecoders.com/blog/wp-content/uploads/2016/08/react.png\" className=\"card-img-top\" alt=\"...\"/>\n            <div className=\"card-body\">\n                <h5 className=\"card-title\">\n                    {\n                        !editing &&\n                        <Link to={`/courses/grid/edit/${course._id}`}>\n                            {course.title}\n                        </Link>\n                    }\n                    {\n                        editing &&\n                        <input\n                            className=\"form-control\"\n                            onChange={(e) => setTitle(e.target.value)}\n                            value={title}/>\n                    }\n                </h5>\n                <p className=\"card-text\">Some quick example text to build on the card title and make up the bulk of\n                    the card's content.</p>\n                <Link to=\"{`/courses/grid/edit/${course._id}`}\" className=\"btn btn-primary\">\n                    Go somewhere\n                </Link>\n            </div>\n        </div>)\n}\n\nexport default CourseCard\n","/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/reducers/module-reducer.js",[],"/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/reducers/topic-reducer.js",[],"/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/reducers/lesson-reducer.js",[],"/Users/liuxingbo/webdev/2021/spring/02/wbdv-sp21-02-xingboliu-client-react/src/components/course-editor/course-editor.js",["104","105","106","107","108"],"import {combineReducers, createStore} from \"redux\";\nimport lessonReducer from \"../../reducers/lesson-r